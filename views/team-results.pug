html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Trivia
        link(rel='stylesheet', href='/stylesheets/home.css')
        script(src='/javascripts/navbar.js' defer)
    body
        header
            include navbar.pug
        each team in teams
            img(src=team.logo, alt=team.name, width="20px")
            a#link(href="team/"+ team._id style="color: black;")= team.teamName + "("+ team.season+")"
            br
            br